<div id="signinWrapper" class="authRegister">
  <a href="http://www.cozyhr.com"><div id="logo"></div></a>
  <div id="pageSignin">
    <div class="intro-company">Welcome to CozyHR!</div>
    <div class="login-form">
      <div><input type="text" id="companyName" placeholder="Company Name"></div>
      <div><input type="text" id="email" placeholder="Your Company Email"></div>
      <div><input type="text" id="subdomain" placeholder="Company Subdomain"></div>
      <div><input type="text" id="nameFirst" placeholder="First Name"></div>
      <div><input type="text" id="nameLast" placeholder="Last Name"></div>
      <div><input type="password" id="passwordOne" placeholder="Password"></div>
      <div><input type="password" id="passwordTwo" placeholder="Password Again"></div>
    </div>
    <div class="press-enter">Press enter to start enjoying the benefits of CozyHR.</div>
  </div>
</div>